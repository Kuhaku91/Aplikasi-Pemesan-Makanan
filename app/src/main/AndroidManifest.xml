<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="hariyanto.alif.pamalayu">

    <uses-permission android:name="android.permission.INTERNET" />

    <application xmlns:tools="http://schemas.android.com/tools"
        android:allowBackup="true"
        android:icon="@drawable/donuts"
        android:label="@string/app_name"
        android:roundIcon="@drawable/donuts"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:replace="android:icon">
        <activity android:name="hariyanto.alif.pamalayu.OrderStatus"></activity>
        <activity android:name="hariyanto.alif.pamalayu.Cart" />
        <activity android:name="hariyanto.alif.pamalayu.FoodDetail" />
        <activity android:name="hariyanto.alif.pamalayu.FoodList" />
        <activity
            android:name="hariyanto.alif.pamalayu.Home"
            android:label="@string/title_activity_home"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name="hariyanto.alif.pamalayu.SignUp" />
        <activity android:name="hariyanto.alif.pamalayu.SignIn" />
        <activity android:name="hariyanto.alif.pamalayu.SplashScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="hariyanto.alif.pamalayu.MainActivity" />
    </application>

</manifest>